@{
    Layout = null;

}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title>VJShop</title>


    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/css/metisMenu.css" />
    <link rel="stylesheet" href="~/Content/css/style.css" />
    <link rel="stylesheet" href="~/Content/css/colors/default.css" id="colorSkinCSS" />
</head>
<body class="crm_body_bg">




    <nav class="sidebar">
        <div class="logo d-flex justify-content-between">
            <a href="index.html"><img src="~/Content/img/VJShop.jpg" alt="" /></a>
            <div class="sidebar_close_icon d-lg-none">
                <i class="ti-close"></i>
            </div>
        </div>
        <ul id="sidebar_menu">
            <li class="mm-active">
                <a class="has-arrow" href="#" aria-expanded="false">

                    <img src="~/Content/img/menu-icon/1.svg" />
                    <span>Menu quản trị</span>
                </a>
                <ul>
                    <li><a class="active">@Html.ActionLink("Máy ảnh", "Index", "mAYANHs")</a></li>

                </ul>

            </li>



        </ul>

    </nav>
    <section class="main_content dashboard_part">

        <div class="container-fluid no-gutters">
            <div class="row">
                <div class="col-lg-12 p-0">
                    <div class="header_iner d-flex justify-content-between align-items-center">
                        <div class="sidebar_icon d-lg-none">
                            <i class="ti-menu"></i>
                        </div>
                        <div class="serach_field-area">
                            <div class="search_inner">
                                <form action="#">
                                    <div class="search_field">
                                        <input type="text" placeholder="Search here..." />
                                    </div>
                                    <button type="submit"> <img src="~/Content/img/icon/icon_search.svg" alt="" /> </button>
                                </form>
                            </div>
                        </div>
                        <div class="header_right d-flex justify-content-between align-items-center">
                            <div class="header_notification_warp d-flex align-items-center">
                                <li>
                                    <a href="#"> <img src="~/Content/img/icon/bell.svg" alt="" /> </a>
                                </li>
                                <li>
                                    <a href="#"> <img src="~/Content/img/icon/msg.svg" alt="" /> </a>
                                </li>
                            </div>
                            <div class="profile_info">
                                <img src="~/Content/img/avatar.jpg" alt="#" />
                                <div class="profile_info_iner">
                                    <p>Welcome Admin!</p>
                                    <h5>Admin</h5>
                                    <div class="profile_info_details">

                                        <a href="https://localhost:44392/Admin/Admin/Index">Đăng nhập <i class="ti-shift-left"></i></a>
                                        <a href="https://localhost:44392/Admin/Admin/Index">Đăng xuất <i class="ti-shift-left"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="main_content_iner ">
            @RenderBody()
            @RenderSection("scripts", required: false)
        </div>
        
        <div class="footer_part">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="footer_iner text-center">
                            <p>Hà Nội - Hà Nội - Việt Nam <a href="#"> <i class="ti-heart"></i> </a><a href="#">- 2021</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>


</body>
</html>
